<!-- Content -->
	<div class="index-container">
		<div class="text-center">
		
			<img class="server-avatar" src="<%= Guild.iconURL %>" onerror="this.onerror=null;this.src='https://i.imgur.com/2otMem9.png';">
			<p class="server-name" title="<%= Guild.name %>"><%= Guild.name %></p>
			<ul class="tags">

<% for(var i=0; i < Tags.length; i++) { %> 
        <% if(Tags === "") return; %>
				<a class="btn btn-success" href="/">
					<%- Tags[i] %>
				</a>
<% } %>

			</ul>
		
		</div>
	</div>

	<div class="container">
		<div class="row server-container">
		  <div class="col-lg-3">
		  
			<div class="card server-info">
			  <div class="card-body">
				<h5 class="le card-title" styele="font-size:25px;">Server Info</h5>
				<ul class="list-group list-group-flush">
				  <li class="le list-group-item">Status: <%= Status %></li>
				  <li class="le list-group-item">Members: <%= Guild.memberCount %></li>
				  <li class="le list-group-item">Online: <!--Guild.members.filter(m => m.presence.status === 'online').size --></li>
          <% if(timeObj === "n/a") { %> <li class="le list-group-item">Last Bump: Never</li> <% } else { %>
          <% if(timeObj.hours === 0) { %> <li class="le list-group-item">Last Bumped: <%= timeObj.minutes %> mins ago</li> <% } else { %> <li class="le list-group-item">Last Bumped: <%= timeObj.hours %> hours ago</li> <% } %>
          <% } %>
				</ul>
			  </div>
			</div>
      <a class="btn btn-bump btn-block <%= Option %>" href="/info/<%= Guild.id %>/bump" style="margin-bottom: 15px; background:#b2c8d2" rel="nofollow" >Bump The Server</a>
			<a class="btn btn-success btn-block <%= Option %>" href="/info/<%= Guild.id %>/invite" style="margin-bottom: 15px;" rel="nofollow" target="_blank">Join the server</a>
		<% if(user){ %>
      <% if(Users.includes(user.id)) { %>
        <a class="btn btn-orange btn-block" onclick=goto('/info/<%= Guild.id %>/edit') style="margin-bottom: 15px;">Edit you're server</a>
        <% } %>
      <% if(Bumper.Developers.includes(user.id)) { %> 
        <% let ver; if(Status === "Official") { ver = "disabled" } if(Status === "Verified") { ver = "disabled" } %>
      <a class="btn btn-gold btn-block <%= ver %>" onclick=goto('/info/<%= Guild.id %>/verify') style="margin-bottom: 15px; background:gold" >Verify the Server</a>
      <% } %>
      <% if(Bumper.Developers.includes(user.id)) { %> 
        <% let ver; if(Status === "Official") { ver = "disabled" } %>
      <a class="btn btn-gold btn-block <%= ver %>" onclick=goto('/info/<%= Guild.id %>/official') style="margin-bottom: 15px; background:gold" >Officalialize the Server</a>
      <% } %>
        <% if(Hidden.includes(Guild.id)) { %> 
        <a class="btn btn-gold btn-block" onclick=goto('/info/<%= Guild.id %>/un-hide') style="margin-bottom: 15px; background:gold">Show you're server</a>
            <% } else { %> 
        <% if(Users.includes(user.id)) { %>
        <a class="btn btn-danger btn-block" onclick=goto('/info/<%= Guild.id %>/hide') style="margin-bottom: 15px;">Hide you're server</a>
        <% } %>
        <% } %> 
    <% } %>
		  </div>
		  <div class="col-lg-9">
                  
<% if(UpV === "123"){ %>
       <br>
<div class="container">
 <center>
   <div class="row" role="alert">
     <div class="col-md-12" role="alert">
       <div class="alert alert-primary alert-success" role="alert">
         <button type="button" class="close" data-dismiss="alert">×</button>
         <h4 class="alert-heading">Done!</h4>
         <p class="mb-0">You Upvoted <%= Guild.name %></p>
      </div>
    </div>
  </div>
 </center>
</div>

<% } %>
        
<% if(UpV === "425"){ %>
       <br>
<div class="container">
 <center>
   <div class="row" role="alert">
     <div class="col-md-12" role="alert">
       <div class="alert alert-primary alert-danger" role="alert">
         <button type="button" class="close" data-dismiss="alert">×</button>
         <h4 class="alert-heading">Oops, there is a problem..</h4>
         <p class="mb-0">Your Server has too many tags please contact <bold>Tea Cup#9999</bold> If you think this is wrong.</p>
      </div>
    </div>
  </div>
 </center>
</div>

<% } %>
        
<% if(min){ %>
       <br>
<div class="container">
 <center>
   <div class="row" role="alert">
     <div class="col-md-12" role="alert">
       <div class="alert alert-primary alert-danger" role="alert">
         <button type="button" class="close" data-dismiss="alert">×</button>
         <h4 class="alert-heading">Oops, there is a problem..</h4>
         <p class="mb-0">You can upvote this server in <bold><%= min %> Minute(s)</bold></p>
      </div>
    </div>
  </div>
 </center>
</div>

<% } %>
        
			<div class="card">
			  <div class="card-body">
				<h5 class="card-title">Server Description</h5>
				<div class="le card-text server-desc"><%- html %></div>
				
			  </div>
			</div>
		  </div>
		</div>
	</div>
  
<%- include('./Blocks/Footer.ejs') %>
  
</html>